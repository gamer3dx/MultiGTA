		} 
		else if(strcomp(Houses[id][Houses_Renter],"Unknown",true) == 1){
			format(string, sizeof(string),gettext(1036),Houses[id][Houses_Owner],Houses[id][Houses_RentCost],Houses[id][Houses_Cost]+Houses[id][Houses_Buyout]);
		}
	}
}
else 
{
	if(strcomp(Houses[id][Houses_Renter],"Unknown",true) == 0){
		format(string,sizeof(string),gettext(1037),Houses[id][Houses_Renter],Houses[id][Houses_Description]);
