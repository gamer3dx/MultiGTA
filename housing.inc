				if (Houses[id][Houses_Upgrade_Armour] > 0){
					new armour = floatround(oGetPlayerArmour(playerid));
					if (armour < 100){
						SetPlayerArmour(playerid,armour + Houses[id][Houses_Upgrade_Armour]);
						SystemMsgFormat(playerid,COLOUR_OK,gettext(1083),Houses[id][Houses_Upgrade_Armour]);
					} else {
						SystemMsg(playerid,COLOUR_ERROR,gettext(1648));
					}	
				}

				new logstring[256];
				format(logstring, sizeof (logstring), "player: %d:  %s: restored in the house",playerid,oGetPlayerName(playerid));
				WriteLog(logstring);
			}
		} else {
			SystemMsg(playerid,COLOUR_ERROR,gettext(1031));
		}
	} else {
		SystemMsg(playerid,COLOUR_ERROR,gettext(1085));
	}
	Debug("housing.inc > Command 'restore' - Stop");
	return 1;
}

