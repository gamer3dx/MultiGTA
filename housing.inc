dcmd_houseupgrade(playerid,params[])
{
	Debug("housing.inc > Command 'houseupgrade' - Start");

	new id=PlayerHouseID[playerid];
	new temp[MAX_STRING];
	if (id > -1)
	{
		if (IsPlayerRegistered(playerid))
		{
			if(strcomp(Houses[id][Houses_Owner], oGetPlayerName(playerid), true) == 0)
			{
				SystemMsg(playerid,COLOUR_ERROR,gettext(1032));
			} else {
				new upgradeid = strval(params);
				if ((upgradeid != 1) && (upgradeid != 2))
				{
					SystemMsg(playerid,COLOUR_INFO,gettext(1368));
					SystemMsgFormat(playerid,COLOUR_INFO,gettext(1369),Houses_Health_Cost);
					SystemMsgFormat(playerid,COLOUR_INFO,gettext(1370),Houses_Armour_Cost);
					Debug("housing.inc > Command 'houseupgrade' - Stop");
					return 1;
				}

				if (upgradeid == 1){
					if (oGetPlayerMoney(playerid) < Houses_Health_Cost){
						SystemMsgFormat(playerid,COLOUR_ERROR,gettext(1647),Houses_Health_Cost);
						Debug("housing.inc > Command 'houseupgrade' - Stop");
						return 1;
					}
					if (Houses[id][Houses_Upgrade_Health] >= 50){
						SystemMsg(playerid,COLOUR_ERROR,gettext(1373));
						Debug("housing.inc > Command 'houseupgrade' - Stop");
						return 1;								
					}
					oGivePlayerMoney(playerid,-Houses_Health_Cost,1);
					Houses[id][Houses_Upgrade_Health] = Houses[id][Houses_Upgrade_Health] + 25;
					new dinifilename[MAX_STRING];
					format(dinifilename,sizeof(dinifilename),"%s%s.txt",Houses_DB,Houses[id][Houses_UID]);
					if (db_Exists(dinifilename))
					{
						valstr(temp,Houses[id][Houses_Upgrade_Health]);
						db_Set(dinifilename,"Upgrade_Health",temp);
						set(temp,nullstr);
					}
					SystemMsgFormat(playerid,COLOUR_OK,gettext(1371),Houses[id][Houses_Upgrade_Health]);
					new logstring[256];
					format(logstring, sizeof (logstring), "player: %d:  %s: upgrade house - health (houseid: %d)",playerid,oGetPlayerName(playerid),id);
					WriteLog(logstring);
				} else {
					if (oGetPlayerMoney(playerid) < Houses_Armour_Cost){
						SystemMsgFormat(playerid,COLOUR_ERROR,gettext(1647),Houses_Armour_Cost);
						Debug("housing.inc > Command 'houseupgrade' - Stop");
						return 1;
					}
					if (Houses[id][Houses_Upgrade_Armour] >= 50){
						SystemMsg(playerid,COLOUR_ERROR,gettext(1373));
						Debug("housing.inc > Command 'houseupgrade' - Stop");
						return 1;								
					}
					oGivePlayerMoney(playerid,-Houses_Armour_Cost,1);
					Houses[id][Houses_Upgrade_Armour] = Houses[id][Houses_Upgrade_Armour] + 25;
					new dinifilename[MAX_STRING];
					format(dinifilename,sizeof(dinifilename),"%s%s.txt",Houses_DB,Houses[id][Houses_UID]);
					if (db_Exists(dinifilename))
					{
						valstr(temp,Houses[id][Houses_Upgrade_Armour]);
						db_Set(dinifilename,"Upgrade_Armour",temp);
						set(temp,nullstr);
					}
					SystemMsgFormat(playerid,COLOUR_OK,gettext(1372),Houses[id][Houses_Upgrade_Armour]);
					new logstring[256];
					format(logstring, sizeof (logstring), "player: %d:  %s: upgrade house - armor (houseid: %d)",playerid,oGetPlayerName(playerid),id);
					WriteLog(logstring);
				}
			}
		} else {
			SystemMsg(playerid,COLOUR_ERROR,gettext(1031));
		}
	} else {
		SystemMsg(playerid,COLOUR_ERROR,gettext(1085));
	}
	Debug("housing.inc > Command 'houseupgrade' - Stop");
	return 1;
}