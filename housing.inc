	if (GetPlayerHouse(playerid) == -1){
		SystemMsg(playerid,COLOUR_ERROR,gettext(1059));
		Debug("housing.inc > Command 'showhouse' - Stop");
		return 1;
	}

	if (PlayerQuest[playerid] != QUEST_SHOWSOMETHING)
	{
		oDisablePlayerCheckpoint(playerid);
		new hid=GetPlayerHouse(playerid);
		oSetPlayerCheckpoint(playerid,Houses[hid][Entrance_X],Houses[hid][Entrance_Y],Houses[hid][Entrance_Z],5);
       		PlayerQuest[playerid] = QUEST_SHOWSOMETHING;
		SystemMsg(playerid,COLOUR_OK,gettext(1102));
	} else {
	    	oDisablePlayerCheckpoint(playerid);
        	PlayerQuest[playerid] = 0;
	}
	Debug("housing.inc > Command 'showhouse' - Stop");
	return 1;
}

