public Housing_OnPlayerSpawn(playerid)
{
	if (Player[playerid][SpawnPlace] == SPAWN_HOUSE_OWNER){
		new house = GetPlayerHouse(playerid);
		if (house == -1){
			Player[playerid][SpawnPlace] = SPAWN_RANDOM; 
		} else {
			oSetPlayerPos(playerid,Houses[house][Entrance_X],Houses[house][Entrance_Y],Houses[house][Entrance_Z]);
		}
	}
	if (Player[playerid][SpawnPlace] == SPAWN_HOUSE_RENTER){
		new rent = GetPlayerRent(playerid);
		if (rent == -1){
			Player[playerid][SpawnPlace] = SPAWN_RANDOM; 
		} else {
			oSetPlayerPos(playerid,Houses[rent][Entrance_X],Houses[rent][Entrance_Y],Houses[rent][Entrance_Z]);
		}
	}
	return 0;
}

