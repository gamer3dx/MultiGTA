public RentMoneyCollect()
{
	for (new id = 0; id < MAX_PLAYERS_EX; id++)
	{
		 if (IsPlayerConnected(id))
		 {
			for (new h = 0; h < MAX_HOUSES; h++)
			{
				if(strcomp(Houses[h][Houses_Renter],oGetPlayerName(id),true) == 1){
					if (oGetPlayerMoney(id) > Houses[h][Houses_RentCost])
					{
						new owner=-1;
						for (new ownerid = 0; ownerid < MAX_PLAYERS_EX; ownerid++)
						{
							if (IsPlayerConnected(ownerid))
							{
								if(strcomp(Houses[h][Houses_Owner], oGetPlayerName(ownerid), true) == 1)
								{
									owner=ownerid;
								}
							}
						}
						oGivePlayerMoney(id,0-Houses[h][Houses_RentCost],1);
						SystemMsgFormat(id,COLOUR_MONEY_BAD,gettext(1105),Houses[h][Houses_RentCost]);
						if (owner > -1)
						{
							oGivePlayerMoney(owner,0+Houses[h][Houses_RentCost],1);
							SystemMsgFormat(id,COLOUR_MONEY_GOOD,gettext(1106),Houses[h][Houses_RentCost]);
						} 
						else 
						{
							new dinifilename[MAX_STRING];
							format(dinifilename,sizeof(dinifilename),"%s%s.txt",Player_DB,EncodeName(Houses[h][Houses_Owner]));
							if (db_Exists(dinifilename))
							{
								new tempmoney;
								tempmoney = strval(db_Get(dinifilename,"BankMoney"));
								tempmoney = tempmoney + Houses[h][Houses_RentCost];
								new temp[MAX_STRING];
								valstr(temp,tempmoney);
								db_Set(dinifilename,"BankMoney",temp);
								set(temp,nullstr);
							}
						}
					}
					else
					{
					set(Houses[h][Houses_Renter],"Unknown");
					Houses[h][Houses_Rentable] = 1;
					new dinifilename[MAX_STRING];
					format(dinifilename,sizeof(dinifilename),"%s%s.txt",Houses_DB,Houses[h][Houses_UID]);
					if (db_Exists(dinifilename))
					{
						new temp[MAX_STRING];
						db_Set(dinifilename,"Renter","Unknown");
						valstr(temp,Houses[h][Houses_Rentable]);
						db_Set(dinifilename,"Rentable",temp);
						set(temp,nullstr);
					}
					SystemMsg(id,COLOUR_ERROR,gettext(1107));
					}
				}
			}
		}
	}
}

public EnterHouse(playerid){
Debug("housing.inc > EnterHouse - Start");
new id=GetHouseEntranceID(playerid);
if (id > -1)
{
	if (IsPlayerRegistered(playerid))
	{
		if(strcomp(Houses[id][Houses_Owner], oGetPlayerName(playerid), true) == 0)
		{
			if(strcomp(Houses[id][Houses_Renter],oGetPlayerName(playerid),true) == 1){
				PlayerHouseID[playerid]=id;
				WhereHeEnter[playerid][Coord_X] = PlayerPos[playerid][Coord_X];
				WhereHeEnter[playerid][Coord_Y] = PlayerPos[playerid][Coord_Y];
				WhereHeEnter[playerid][Coord_Z] = PlayerPos[playerid][Coord_Z];
				SetPlayerInterior(playerid,Houses[id][InteriorID]);
				Player[playerid][WasAtHouse]++;
				oSetPlayerPos(playerid,Houses[id][Exit_X],Houses[id][Exit_Y],Houses[id][Exit_Z]);
				ShowTextForPlayer(playerid,gettext(1108),1000,1);
				SystemMsg(playerid,COLOUR_INFO,gettext(1109));
			} else {
				SystemMsg(playerid,COLOUR_ERROR,gettext(1032));
			}
		} else {
				PlayerHouseID[playerid]=GetHouseEntranceID(playerid);
				WhereHeEnter[playerid][Coord_X] = PlayerPos[playerid][Coord_X];
				WhereHeEnter[playerid][Coord_Y] = PlayerPos[playerid][Coord_Y];
				WhereHeEnter[playerid][Coord_Z] = PlayerPos[playerid][Coord_Z];
				SetPlayerInterior(playerid,Houses[id][InteriorID]);
				Player[playerid][WasAtHouse]++;
				oSetPlayerPos(playerid,Houses[id][Exit_X],Houses[id][Exit_Y],Houses[id][Exit_Z]);
				ShowTextForPlayer(playerid,gettext(1110),1000,1);
				SystemMsg(playerid,COLOUR_INFO,gettext(1111));
				SetPlayerVirtualWorld(playerid,PrivateWorld(playerid));
				new logstring[256];
				format(logstring, sizeof (logstring), "player: %d:  %s: enter the house",playerid,oGetPlayerName(playerid));
				WriteLog(logstring);
			}
		} else {
	SystemMsg(playerid,COLOUR_ERROR,gettext(1031));
}
	} else {
				SystemMsg(playerid,COLOUR_ERROR,gettext(1030));
}
Debug("housing.inc > EnterHouse - Stop");
return 1;
}

public ExitHouse(playerid){
